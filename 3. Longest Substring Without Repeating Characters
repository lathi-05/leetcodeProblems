class Solution {
    public int lengthOfLongestSubstring(String s) {
        int maxLen=0;
        for(int i=0;i<s.length();i++){
            String sub="";
            
            for(int j=i;j<s.length();j++){
                char word=s.charAt(j);
            if(sub.indexOf(word)!=-1){
                break;
            }
            sub+=word;
            maxLen=Math.max(maxLen,sub.length());
            }
        }
        return maxLen;
    }
}
