class Solution {
    public int maxProfit(int[] prices) {
        int maxProfit=0;
        int minPrice=prices[0];
        for(int i=0;i<prices.length;i++){
            int sub=prices[i]-minPrice;
            if(sub>maxProfit){
                maxProfit=sub;
            }
            if(prices[i]<minPrice){
                minPrice=prices[i];
            }
        }
        return maxProfit;
    }
}
